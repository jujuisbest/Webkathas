        </div>
        <% } %>
        <% if(success&& success.length>0){ %>
        <div class="alert alert-success" role="alert">
        <%=success%>
        </div>
        <% } %>
    
	<!-- show any messages that come back with authentication -->
	<% if (message.length > 0) { %>
		<div class="alert alert-danger"><%= message %></div>
	<% } %>
	
	</div>
